<linker>
	<assembly fullname="UnityEngine">

		<!-- Events that come from the OS will trigger callbacks on these objects -->
		<!-- Games should be acting on those events, but we make sure they are not -->
		<!-- stripped so you don't have to worry about them for quick prototypes -->
		<!--type fullname="UnityEngine.PSP2.Input" /-->
	</assembly>
	
	<assembly fullname="mscorlib">
		<type fullname="System.Exception" />
	</assembly>
	<assembly fullname="System.Configuration">
		<namespace fullname="System.Configuration" preserve="all"/>
	</assembly>
	<assembly fullname="System">
		<namespace fullname="System.Net.Configuration" preserve="all"/>
		<namespace fullname="System.ComponentModel" preserve="all"/>
	</assembly>
</linker>
